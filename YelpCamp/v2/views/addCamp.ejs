<% include partials/header.ejs %>
<h1 class="text-center">Create new Camp Ground</h1>
<div class="container">
    <div class="row">
        <div class="campform">
            <form method="POST" action="/campgrounds">
                <div class="form-group">
                    <input class="form-control campfield" type="text" placeholder="name" name="campname">
                </div>
                <div class="form-group">
                    <input class="form-control campfield" type="text" placeholder="image url" name="imagesource">
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">SUBMIT</button>
                </div>
            </form>
            <a href="/campgrounds">Back to campgrounds</a>
            <% include partials/footer.ejs %>
        </div>
    </div>
</div>
