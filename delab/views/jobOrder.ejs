<% include partials/header %>
<form class="form" action="/jobOrder" method="POST"> <!-- Form Start -->
<div class="panel panel-default">
	<div class="panel-heading  joborderpanel">
    	<h3 class="panel-title">CUSTOMER INFORMATION</h3>
	</div>
  	<div class="panel-body">
	    
	    	<div class="row">
	    		<div class="col-xs-4"><!-- customer name -->
			  		<div class="form-group">
			    		<label class="customerinfolabel" for="customerName">Customer Name: </label>
			    		<input type="text" class="form-control" id="customerName" name="customerName">
			 		</div>
			 	</div><!-- customer name -->
			 	<div class="col-xs-4"><!-- deadline -->
			  		<div class="form-group">
			    		<label class="customerinfolabel" for="deadline">Deadline: </label>
			    		<input type="date" class="form-control" id="deadline" name="deadline">
			  		</div>
			  	</div><!-- /deadline -->
		  	</div><!-- /first row -->
		  	<br>
		  	<div class="row">
			 	<div class="col-xs-4">
			    	<div class="form-group">
			    		<label class="customerinfolabel" for="schoolCompany">School / Company:  </label>
			    		<input type="text" class="form-control" id="schoolCompany" name="schoolCompany">
			 		</div>
			  	</div><!-- /.col-lg-6 -->
			  	<div class="col-xs-4"><!-- pickup -->
			  		<div class="form-group">
			    		<label class="customerinfolabel" for="pickup">Pick Up: </label>
			    		<input type="date" class="form-control" id="pickup" name="pickup">
			  		</div>
			  	</div> <!-- /pickup -->
			 </div><!-- /secondrow -->
			 <br>
			 <div class="row">
	    		<div class="col-xs-4">
			  		<div class="form-group">
			    		<label class="customerinfolabel" for="contactNumber">Contact Number: </label>
			    		<input type="text" class="form-control" id="contactNumber" name="contactNumber">
			 		</div>
			 	</div>
			 	<div class="col-xs-4">
			  		<div class="form-group">
			    		<label class="customerinfolabel" for="emailAddress">Email Address </label>
			    		<input type="email" class="form-control" id="emailAddress" name="emailAddress">
			  		</div>
			  	</div>
		  	</div><!-- /third row -->
		  	<br>
			 <div class="row">	
			 	<div class="col-xs-2">	
			 		<label class="customerinfolabel">How did you find us?:</label>
    				<div class="input-group">
      					<span class="input-group-addon">
        					<input name="howdidyoufindus" value="Facebook" type="radio" aria-label="...">
      					</span>
      					<input type="text" class="form-control checkdisabled" value="Facebook" aria-label="..." disabled>
    				</div><!-- /input-group -->
    				<div class="input-group">
      					<span class="input-group-addon">
        					<input name="howdidyoufindus" value="Instagram" type="radio" aria-label="...">
      					</span>
      					<input type="text" class="form-control checkdisabled" value="Instagram" aria-label="..." disabled>
    				</div><!-- /input-group -->
    				<div class="input-group">
      					<span class="input-group-addon">
        					<input name="howdidyoufindus" type="radio" aria-label="...">
      					</span>
      					<input type="text" class="form-control" placeholder="Referred by..." aria-label="...">
    				</div><!-- /input-group -->
			 	</div>
			 </div><!-- /fourth row -->
  	</div>
</div><!-- /first panel -->
<div class="panel panel-default">
	<div class="panel-heading  joborderpanel">
